<com:TContent ID="MainContent">
	<div class="propertyWrapper">
	    <com:TCallback ID="getPropertiesBtn" OnCallback="Page.getProperties"/>
	    <com:TCallback ID="savePropertyBtn" OnCallback="Page.saveProperty"/>
	    <com:TCallback ID="delPropertyBtn" OnCallback="Page.delProperty"/>
	    <com:TCallback ID="getAccBtn" OnCallback="Page.getAccEntries"/>
	    <div id="transResults" class="content-box">
	        <h3 class="box-title">
	            <span class='noOfItems'>0</span> Property Found!
	            <input type="button" value="Create" class="submitBtn addBtn"/>
	        </h3>
	        <div id="propertyList" class="box-content resultlist">
	        </div>
	    </div>
	</div>
	<com:Application.controls.FileUploader.FileUploader>
        var pageJs = new PropertiesJs('propertyList', 
            '<%= $this->getPropertiesBtn->getUniqueID()%>', 
            '<%= $this->savePropertyBtn->getUniqueID()%>', 
            '<%= $this->delPropertyBtn->getUniqueID()%>', 
            '<%= $this->getAccBtn->getUniqueID()%>');
        pageJs.loadProperties();
    </com:Application.controls.FileUploader.FileUploader>
</com:TContent>