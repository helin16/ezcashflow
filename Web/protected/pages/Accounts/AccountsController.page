<com:TContent ID="MainContent">
    <com:TCallback ID="getAccountsBtn" OnCallback="Page.getAccounts"/>
    <div class="content-box listheader">
        <h3 class="box-title">
            <ul>
                <li><a onclick="return pageJs.showAccounts(this);" rootId="1" selected>Assets</a></li>
                <li><a onclick="return pageJs.showAccounts(this);" rootId="2">Liability</a></li>
                <li><a onclick="return pageJs.showAccounts(this);" rootId="3">Income</a></li>
                <li><a onclick="return pageJs.showAccounts(this);" rootId="4">Expense</a></li>
            </ul>
        </h3>
        <div class="box-content">
            <div id="accountList">
            </div>
        </div>
    </div>
    <com:TClientScript>
        var pageJs = new AccountsJs('accountList', '<%= $this->getAccountsBtn->getUniqueId()%>');
        $$(".content-box.listheader .box-title ul li a[selected]").first().click();
    </com:TClientScript>
</com:TContent>